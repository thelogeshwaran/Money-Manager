{
    "version": 2,
    "builds": [
      {
        "src": "Backend/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "OPTIONS",
          "DELETE"
        ]
      }
    ]
  }